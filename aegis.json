{
	"host": "http://localhost",
	"port": ":8081",
	"endpoints": [{
		"plugins": [{
			"type": "monitor",
			"settings": {
				"verbose": true
			}
		},
		{
			"type":"ipfilter",
			"settings":{
				"blacklist": ["::1", "127.0.0.1"],
				"whitelist": []
			}
		}],
		"methods": ["GET"],
		"external": "/external",
		"internal": "/internal"
	}]
}
